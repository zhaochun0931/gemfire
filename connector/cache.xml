<?xml version="1.0" encoding="UTF-8" standalone="no"?><cache version="1.0" xsi:schemaLocation="http://geode.apache.org/schema/jdbc http://geode.apache.org/schema/jdbc/jdbc-1.0.xsd gpdb http://schema.pivotal.io/gemfire/gpdb/gpdb-4.0.xsd gpdb http://schema.pivotal.io/gemfire/gpdb/gpdb-4.0.xsd http://geode.apache.org/schema/query-config-service http://geode.apache.org/schema/query-config-service/query-config-service-1.0.xsd http://geode.apache.org/schema/cache http://geode.apache.org/schema/cache/cache-1.0.xsd" xmlns="http://geode.apache.org/schema/cache" xmlns:query-config-service="http://geode.apache.org/schema/query-config-service" xmlns:gpdb="http://schema.pivotal.io/gemfire/gpdb" xmlns:jdbc="http://geode.apache.org/schema/jdbc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <pdx read-serialized="true" ignore-unread-fields="false" persistent="false">
        <pdx-serializer>
            <class-name>org.apache.geode.pdx.ReflectionBasedAutoSerializer</class-name>
            <parameter name="classes">
                <string>io.pivotal.gemfire.demo.entity.*</string>
            </parameter>
        </pdx-serializer>
    </pdx>
    <jndi-bindings>
        <jndi-binding connection-url="jdbc:postgresql://test:5432/test" jdbc-driver-class="org.postgresql.Driver" jndi-name="datasource" password="changeme" type="SimpleDataSource" user-name="gpadmin"/>
    </jndi-bindings>
    <region name="t1" refid="PARTITION">
        <region-attributes data-policy="partition"/>
        <gpdb:store datasource="datasource" writable-external-table-protocol="gpfdist">
            <gpdb:pdx name="io.pivotal.gemfire.demo.entity.t1" schema="public" table="t1">
                <gpdb:id>
                    <gpdb:field ref="id"/>
                </gpdb:id>
            </gpdb:pdx>
        </gpdb:store>
    </region>
    <region name="t2" refid="PARTITION">
        <region-attributes data-policy="partition"/>
        <gpdb:store datasource="datasource" writable-external-table-protocol="gpfdist">
            <gpdb:pdx name="io.pivotal.gemfire.demo.entity.t2" schema="public" table="t2">
                <gpdb:id>
                    <gpdb:field ref="id"/>
                    <gpdb:field ref="name"/>
                </gpdb:id>
            </gpdb:pdx>
        </gpdb:store>
    </region>
    <gpdb:gpfdist port="8848" idle-timeout="60000"/>
